{
	"compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "es6",
		"target": "es5",
		"allowJs": true,
		"baseUrl": "src",
		"paths": {
			"*": ["src/types/*"],
			"@*": ["src/*"]
		},
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true
	}
